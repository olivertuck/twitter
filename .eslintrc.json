{
  "extends": [
    "next/core-web-vitals",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "plugins": ["jsx-a11y", "sort-destructure-keys"],
  "rules": {
    "arrow-body-style": 2,
    "consistent-return": 2,
    "import/prefer-default-export": 2,
    "no-shadow": 2,
    "react/jsx-sort-props": 2,
    "sort-destructure-keys/sort-destructure-keys": 2,
    "sort-imports": 2,
    "sort-keys": 2
  }
}
